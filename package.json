{
    "name": "e-marketplace",
    "version": "1.0.0",
    "description": "this is a marketplace where people can buy and sell various items",
    "main": "server/server.js",
    "scripts": {
      "start": "node server/server.js",
      "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
      "install": "cd server && npm i && cd ../client && npm i",
      "seed": "cd server && npm run seed",
      "build": "cd client && npm run build"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/harry-100/e-marketplace"
    },
    "author": "",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/harry-100/e-marketplace/issues"
    },
    "homepage": "https://github.com/harry-100/e-marketplace#readme",
    "devDependencies": {
      "concurrently": "^6.4.0"
    }
  }